, 'show'], ['driving force', 'plot', 'themes of family', 'dynamics', 'relationship between Peter Quill', 'Ego'], ['story', 'portion of time', 'relationship between Peter', 'father', 'accounts', 'resonating sequences'], ['relationship Gamora', 'Nebula', 'Karen Gillian'], ['family dynamics', 'tonal shifts', 'picture'], ['side', 'film', 'adrenaline tank', 'action sequences', 'spaceship battles', 'CGI explosions', 'things', 'reasonable pace'], ['previous movie', 'backdrop', 'scene', 'soundtrack', 'cheeky mood', 'story'], ['majority', 'cast', 'installment returns'], ['Chris Pratt', 'likable guy', 'returns', 'sheer amount', 'charisma and humor', 'role'], ['performance serves', 'factor', 'chemistry', 'Kurt Russell', 'father'], ['share', 'strong emotional chemistry', 'driving force', 'story'], ['fans of Russell', 'actor', 'disappoints', 'screen time', 'final act'], ['Zoe Saldana', 'role', 'amount of humanity', 'bond with Karen Gillian', 'Nebula'], ['Bradley Cooper', 'voice', 'raccoon Rocket', 'hilarious one-liner'], ['Professional wrestler Dave Batista', 'great effort', 'role of Drax', 'humor', 'sentimental value', 'comedic figure'], ['Groot', 'form', 'voice of Vin Diesel', 'last film', 'departure', 'signature lines'], ['little character', 'screen', 'fails', 'charm'], ['Michael Rooker', 'job as Yondu', 'character'], ['Sylvester Stallone', 'surprise appearance', 'screen time', 'minutes'], ['Pom Klementieff', 'telepathic mantis', 'slice of humanity', 'character', 'pinch', 'chemistry with Dave Batista', 'Drax'], ['anything', 'clever chemistry', 'characters', 'long separation', 'characters with Peter', 'Gamora', 'Drax', 'amount of time', 'planet', 'Rocket and Groot', 'hilarious banter', 'character', 'likability'], ['Guardians', 'Galaxy', 'Volume', 'fun follow-up', 'amount of energy', 'heart', 'humor', 'moviegoers', 'various demographics'], ['previous entry', 'insane amount', 'fun', 'movie'], ['movie', 'summer blockbuster', 'box office receipt', 'end', 'summer']]"
