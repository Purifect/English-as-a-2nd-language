at film', 'making'], [], ['year', 'old daughter', 'something'], [], ['the main attraction'], ['the same exact plot', 'Good versus', 'Evil'], ['minute', 'this pablum', 'a year'], ['woeful trash', 'the film', 'industry'], ['Stanley Kubrick', 'the master', 'grave'], ['this trash'], ['Abel Ferrara', 'Marty Scorcese', 'Ridley Scott', 'William Friedkin', 'Spike Lee', 'Denis Villeneuve', 'a movie'], ['all publicity'], ['much money', 'the opening', 'nothing'], [], ['novacaine', 'watch'], [], ['No way', 'Bobby DeNiro', 'Christopher Walken', 'Matt Damon', 'Colin Farrell', 'Ethan Hawke', 'Nicole Kidman', 'Al Pacino'], [], [], ['integrity'], ['Skip', 'this woeful', 'trash', 'a real film', 'a cartoon']]","[['leap', 'things'], ['cumulative effect', 'super heroes', 'film fan', 'white man', 'Native Americans', 'End of life'], ['actors', 'prostitutes', 'studios'], ['great actors and directors', 'cartoon films'], ['antithesis', 'film making'], [], ['year', 'old daughter', 'something'], ['mature people', 'real films'], ['cartoons', 'main attraction'], ['masses', 'same exact plot', 'versus Evil'], ['minute', 'pablum', 'year'], ['woeful trash', 'film industry'], ['Stanley Kubrick', 'master', 'great directors', 'grave'], ['top directors', 'trash'], ['People like Abel Ferrara', 'Marty Scorcese', 'Ridley Scott', 'William Friedkin', 'Spike Lee', 'Denis Villeneuve', 'movie'], ['publicity'], ['much money', 'opening weeks', 'nothing', 'films'], [], ['teeth', 'novacaine than watch'], ['actors'], ['way', 'people like Bobby DeNiro', 'Christopher Walken', 'Matt Damon', 'Colin Farrell', 'Ethan Hawke', 'Nicole Kidman', 'Al Pacino', 'movies'], [], [], ['integrity'], ['Skip', 'woeful', 'trash', 'real film', 'cartoon']]"
